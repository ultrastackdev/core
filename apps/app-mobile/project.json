{
  "name": "app-mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-mobile",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project app-mobile --web",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": ["{projectRoot}/out"],
      "options": {
        "script": "build"
      }
    },
    "export": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "node -e \"console.log('Using output: export; skipping export step')\""
      },
      "outputs": ["{projectRoot}/out"]
    },
    "add": {
      "executor": "@nxext/capacitor:cap",
      "configurations": {
        "android": {
          "cmd": "add android"
        },
        "ios": {
          "cmd": "add ios"
        }
      }
    },
    "sync": {
      "executor": "@nxext/capacitor:cap",
      "configurations": {
        "android": {
          "cmd": "sync android"
        },
        "ios": {
          "cmd": "sync ios"
        }
      }
    },
    "open": {
      "executor": "@nxext/capacitor:cap",
      "configurations": {
        "android": {
          "cmd": "open android"
        },
        "ios": {
          "cmd": "open ios"
        }
      }
    },
    "serve": {
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx --yes serve -s apps/app-mobile/out -l 3000"
      }
    }
  }
}
